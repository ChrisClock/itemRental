{
  "name": "libnode",
  "version": "0.0.0",
  "authors": [
    "Chris Clock <me@chrisclock.com>"
  ],
  "moduleType": [
    "node"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "jquery": "2.x.x",
    "bootstrap": "~3.x.x",
    "datatables": "~1.x.x",
    "html5sortable": "*",
    "papaparse": "4.1.x"
  },

  "bowerMove": {
    "jquery": {
      "dist/jquery.min.js": "public/javascripts/"
    },
    "bootstrap": {
      "less/[!v]*.less" : "styles/",
      "less/mixins/*.less": "styles/mixins/",
      "dist/fonts/*": "public/fonts/",
      "dist/js/bootstrap.min.js": "public/javascripts/"
    },
    "datatables": {
      "media/css/jquery.dataTables.min.css": "public/stylesheets/",
      "media/images/*.png": "public/images/",
      "media/js/jquery.dataTables.min.js": "public/javascripts/"
    },
    "html5sortable": {
      "jquery.sortable.min.js": "public/javascripts/"
    },
    "papaparse": {
      "papaparse.min.js": "public/javascripts/"
    }
  }
}
